{
    "name": "bobdenotter/example-field",
    "description": "📝 Bolt Extension to add the 'Example Field' FieldType",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "require": {
        "php": ">=7.2.9"
    },
    "require-dev": {
        "bolt/core": "^4.0.0",
        "symplify/easy-coding-standard": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Bobdenotter\\ExampleField\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "Bobdenotter\\ExampleField\\Extension",
        "screenshots": [
            "screenshots/notices.png"
        ]
    }
}
